<script type="text/javascript">
    mui.init()
    var html = "";
    var $mask = $(".mui-backdrop");
    var id = GetQuery("id")
    window.onload = function () {
        getZbjqInfo();
    }
    function getZbjqInfo() {
        var html = "";
        $.ajax({
            url: "../homePage/ZbjqInfo",
            type: "post",
            data: {id: id},
            dataType: "json",
            timeout: "10000",
            success: function (data) {
                $mask.hide();
                if (data.itemSfzs == 1 && data.itemSfwl == 0) {
                    var name = data.itemName;
                    $("title").text(name)
                    $(".mui-title").text(name);
                    var src = domain;
                    var reg = /[sS][rR][cC][\s]*=[\s]*['"]([\s\S]*?)['"]/gi;
                    var s = data.itemDetail.replace(reg, function (match, capture) {
                        return match.replace(capture, src + capture);
                    });
                    $('.mo2').append(s);
                    var img = '',html='';
                    if (data.itemCover) {
                        img = JSON.parse(data.itemCover)[0].path;
                        html = '<img class="mo1" src="' + imgpre + img + '">';
                    } else {
                        img = "../images/attractions.png"
                        html = '<img class="mo1" src="' + img + '">';
                    }
                    $(".mo1").append(html);
                }
            },
            error: function (e) {
                  ("查询失败");
            }
        });
    }
